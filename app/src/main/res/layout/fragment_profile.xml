<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:ignore="ExtraText">

    <EditText
        android:id="@+id/edit1"
        android:layout_width="345dp"
        android:layout_height="400dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:enabled="false"
        android:gravity="center">


        <requestFocus />
    </EditText>

    <TextView
        android:id="@+id/pr_nick"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:text=" 닉네임"
        android:textSize="40sp"
        android:textStyle="bold"></TextView>

    <TextView
        android:id="@+id/pr_lec_renew"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_below="@+id/pr_nick"
        android:layout_centerHorizontal="true"
        android:text="  과목갱신"
        android:textSize="27sp"></TextView>

    <TextView
        android:id="@+id/pr_nick_renew"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_below="@+id/pr_lec_renew"
        android:layout_centerHorizontal="true"
        android:text="  닉네임 변경"
        android:onClick="onTextViewClicked"
        android:clickable="true"
        android:textSize="27sp"></TextView>

    <TextView
        android:id="@+id/pr_pw_renew"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_below="@+id/pr_nick_renew"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="onTextViewClicked"
        android:text="  비밀번호 변경"
        android:textSize="27sp"></TextView>
    <TextView
        android:id="@+id/pr_logout"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_below="@+id/pr_pw_renew"
        android:layout_centerHorizontal="true"
        android:text="  로그아웃"
        android:textSize="27sp">
    </TextView>


    <EditText
        android:id="@+id/edit2"
        android:layout_width="345dp"
        android:layout_height="200dp"
        android:layout_below="@id/edit1"
        android:layout_centerInParent="true"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:enabled="false"
        android:gravity="center">/>


        <requestFocus />
    </EditText>


    <TextView
        android:id="@+id/pr_gitar"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_above="@+id/pr_annae"

        android:clickable="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:text=" 기타"
        android:textSize="40sp"
        android:textStyle="bold"></TextView>

    <TextView
        android:id="@+id/pr_gitarmoon"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_alignBottom="@+id/edit2"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="onTextViewClicked"
        android:text="  기타문의"
        android:textSize="27sp"></TextView>

    <TextView
        android:id="@+id/pr_annae"
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_above="@+id/pr_gitarmoon"
        android:layout_centerHorizontal="true"
        android:text="  이용안내"
        android:onClick="onTextViewClicked"
        android:clickable="true"
        android:textSize="27sp"></TextView>


</RelativeLayout>